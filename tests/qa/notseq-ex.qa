# change example to use globins.fasta
ID notseq-ex
UC In this case the excluded sequences (myg_phyca and lgb2_luplu) are not saved to any file:
AP notseq
IN ../../data/globins.fasta
IN myg_phyca,lgb2_luplu
IN mydata.seq
FI stderr
FC = 2
FP 0 /Warning: /
FP 0 /Error: /
FP 0 /Died: /
FI mydata.seq
FZ = 926
FP /^>HBA_HUMAN/
FP /^>HBA_HORSE/
FP /^>HBB_HUMAN/
FP /^>HBB_HORSE/
FP /^>GLB5_PETMA/
FP 0 /^>MYG_PHYCA/
FP 0 /^>LGB2_LUPLU/
//
